{
  "name": "mazipan",
  "version": "1.0.0",
  "description": "A personal blog by Irfan Maulana",
  "author": "Irfan Maulana",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "generate:ci": "yarn generate:draft && yarn generate:categories && yarn generate:doc && nuxt generate && yarn generate:rss",
    "generate:schema": "node ./build-scripts/schema.js",
    "generate:rss": "node ./build-scripts/rss.js",
    "generate:draft": "node ./build-scripts/drafts.js",
    "generate:categories": "node ./build-scripts/categories.js",
    "generate:doc": "cp README.md static/README.md",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .eslintignore . --fix",
    "lint:report": "eslint --ext .js,.vue --ignore-path .eslintignore . -f html-extended -o eslint-report.html",
    "lint:md": "remark readme.md -o --color",
    "test": "jest",
    "lh": "lighthousebot"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "1.3.0",
    "@nuxtjs/google-analytics": "2.2.0",
    "@nuxtjs/pwa": "3.0.0-beta.16",
    "@nuxtjs/sitemap": "1.2.0",
    "firebase": "6.1.1",
    "modern-normalize": "0.5.0",
    "nuxt": "2.8.1",
    "nuxt-webfontloader": "1.1.0",
    "prism-themes": "1.1.0",
    "prismjs": "1.16.0",
    "vue-ionicons": "3.0.4",
    "vue-tiny-lazyload-img": "0.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-jsx": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "9.6.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-plugin-dynamic-import-node-babel-7": "2.0.7",
    "babel-plugin-prismjs": "1.0.2",
    "cross-env": "5.2.0",
    "dotenv": "8.0.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-formatter-html-extended": "1.0.2",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "frontmatter-markdown-loader": "1.7.0",
    "fs-extra": "8.0.1",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "jest-serializer-vue": "2.0.2",
    "lighthousebot": "https://github.com/GoogleChromeLabs/lighthousebot",
    "markdown-parse": "0.2.1",
    "node-sass": "4.12.0",
    "remark-cli": "6.0.1",
    "remark-preset-lint-consistent": "2.0.2",
    "remark-preset-lint-markdown-style-guide": "2.1.2",
    "remark-preset-lint-recommended": "3.0.2",
    "rss": "1.2.2",
    "sass-loader": "7.1.0",
    "vue-jest": "3.0.4",
    "vue-router": "3.0.6"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}
