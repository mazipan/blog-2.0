{
  "name": "mazipan",
  "version": "1.0.0",
  "description": "A personal blog by Irfan Maulana",
  "author": "Irfan Maulana",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "generate": "yarn generate:draft && yarn generate:doc && nuxt generate && yarn generate:rss",
    "generate:schema": "node ./schema.js",
    "generate:rss": "node ./rss.js",
    "generate:draft": "node ./drafts.js",
    "generate:doc": "cp README.md static/README.md",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:report": "eslint --ext .js,.vue --ignore-path .gitignore . -f html-extended -o eslint-report.html",
    "lint:md": "remark readme.md -o --color",
    "test": "jest",
    "lh": "lighthousebot"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/google-analytics": "^2.1.0",
    "@nuxtjs/pwa": "^3.0.0-beta.8",
    "@nuxtjs/sitemap": "^0.2.0",
    "firebase": "^5.9.2",
    "intersection-observer": "^0.6.0",
    "modern-normalize": "0.5.0",
    "nuxt": "^2.4.3",
    "nuxt-webfontloader": "^1.0.0",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "vue-ionicons": "^3.0.4",
    "vue-tiny-lazyload-img": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.4.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-dynamic-import-node-babel-7": "2.0.7",
    "babel-plugin-prismjs": "^1.0.2",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-html-extended": "^1.0.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "frontmatter-markdown-loader": "^1.6.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^2.0.2",
    "lighthousebot": "https://github.com/GoogleChromeLabs/lighthousebot",
    "markdown-parse": "^0.2.1",
    "node-sass": "^4.11.0",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "rss": "^1.2.2",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.3",
    "vue-router": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}
